//
// Data types
//
// Command:
// goa

package types

import 	goa "goa.design/goa/v3/pkg"

// My type
type MyType struct {
	// Age
	Age int
	// Name
	Name string
}
// ValidateMyType runs the validations defined on MyType
func ValidateMyType(v *MyType) (err error) {
	if v.Age < 0 {
        err = goa.MergeErrors(err, goa.InvalidRangeError("v.age", v.Age, 0, true))
}
	return
}


